{
    "extends": "airbnb",
    "env": {
        "node": true,
        "mocha": true
    },
    "rules": {
        "max-len": ["error", {
            "code": 400
        }],
        "indent": ["error", 4],
        "no-param-reassign": 0,
        "func-names": 0,
        "consistent-return": 0,
        "no-shadow": 0,
        "global-require": 0
    }
}
